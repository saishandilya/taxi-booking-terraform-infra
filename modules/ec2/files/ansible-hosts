[jenkins_master]
${aws_instance.jenkins_master.private_ip}

[jenkins_master:vars]
ansible_user=ubuntu
ansible_ssh_private_key_file=/opt/${var.instance_key_name_value}.pem

[jenkins_slave]
${aws_instance.jenkins_slave.private_ip}

[jenkins_slave:vars]
ansible_user=ubuntu
ansible_ssh_private_key_file=/opt/${var.instance_key_name_value}.pem